/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 * takes many inputs and directs to single output
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    And(a=a, b=notsel, out=v1);
    And(a=b, b=sel, out=v2);
    Or(a=v1, b=v2, out=out);
}